@model Book[]

@{
    ViewData["Title"] = "Searching results";
}

<h1>Searching results</h1>

@if (Model.Length == 0)
{
    <p>Nothing found</p>
}
else
{
    <ul>
        @foreach (var book in Model)
        {
            <li><a href="@Url.Action("Index", "Book", new { id = book.Id })">@book.Title</a></li>
        }
    </ul>
}

    
